{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 1
    },
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "$": true,
        "App": true
    }
}
